op/sei/unit4/project/receipts-frontend/src/components/Detail/Detail.js",["67","68","69","70","71"],"// // Will display detailed information about an individual receipt. \n\nimport React, { useState, useEffect } from 'react';\nimport './detail.css';\nimport axios from 'axios';\nimport { Redirect, useHistory } from 'react-router-dom';\nimport APIURL from '../../config';\n\nconst receiptInfo = `${URL}/userreceipts`;\n\nconst Detail = ({ match }) => {\n\tconst history = useHistory();\n\tconst [detail, setDetail] = useState('');\n\tconst [receipt, setReceipt] = useState('');\n\tuseEffect(() => {\n\t\tconst url = `${receiptInfo}${match.params.id}`;\n\t\tconsole.log(url);\n\t\taxios(url)\n\t\t\t.then((res) => {\n\t\t\t\tsetDetail(res.data);\n\t\t\t\tsetReceipt(res.data);\n\t\t\t})\n\t\t\t.catch((err) => {\n\t\t\t\tconsole.error(err);\n\t\t\t});\n\t}, [match.params.id]);\n\n\t// Delete a Submission\n\tconst handleDelete = (event) => {\n\t\tconst url = `${receiptInfo}${match.params.id}`;\n\t\taxios({\n\t\t\turl: url,\n\t\t\tmethod: 'DELETE',\n\t\t})\n\t\t\t.then(() => {\n\t\t\t\thistory.push('/');\n\t\t\t})\n\t\t\t.catch((err) => {\n\t\t\t\tconsole.error(err);\n\t\t\t});\n\t};\n\n\t// // Update a receipt\n\n\tconst handleSubmit = (event) => {\n\t\tevent.preventDefault();\n\t\tconst url = `${receiptInfo}${match.params.id}`;\n\t\taxios({\n\t\t\turl: url,\n\t\t\tmethod: 'PUT',\n\t\t\tdata: detail,\n\t\t}).then(() => {\n\t\t\thistory.push('/');\n\t\t});\n\t};\n\n\tconst handleChange = (event) => {\n\t\tevent.persist();\n\t\tsetDetail({ ...detail, [event.target.name]: event.target.value });\n\t};\n\n\treturn (\n\t\t<div>\n\t\t\t<button className='pretty-button' onClick={handleDelete}>\n\t\t\t\tDelete Receipt\n\t\t\t</button>\n\t\t</div>\n\t);\n};\n\nexport default Detail;",["72","73"],"/Users/owner/Desktop/sei/unit4/project/receipts-frontend/src/config.js",[],"/Users/owner/Desktop/sei/unit4/project/receipts-frontend/src/FileUploader.js",[],"/Users/owner/Desktop/sei/unit4/project/