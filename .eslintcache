ext'\n\t\t\t\t\t\t\tid='username'\n\t\t\t\t\t\t\tvalue={user.username}\n\t\t\t\t\t\t\tonChange={handleChange}\n\t\t\t\t\t\t/>\n\t\t\t\t\t</div>\n\n\t\t\t\t\t<div className='form-component'>\n\t\t\t\t\t\t<label htmlFor='email'>Email: </label>\n\t\t\t\t\t\t<input\n\t\t\t\t\t\t\trequired\n\t\t\t\t\t\t\ttype='email'\n\t\t\t\t\t\t\tid='email'\n\t\t\t\t\t\t\tvalue={user.email}\n\t\t\t\t\t\t\tonChange={handleChange}\n\t\t\t\t\t\t/>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div className='form-component'>\n\t\t\t\t\t\t<label htmlFor='password'>Password: </label>\n\t\t\t\t\t\t<input\n\t\t\t\t\t\t\trequired\n\t\t\t\t\t\t\tonChange={handleChange}\n\t\t\t\t\t\t\ttype='password'\n\t\t\t\t\t\t\tid='password'\n\t\t\t\t\t\t\tvalue={user.password}\n\t\t\t\t\t\t/>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div className='form-component'>\n\t\t\t\t\t\t<label htmlFor='re_password'>Confirm Password: </label>\n\t\t\t\t\t\t<input\n\t\t\t\t\t\t\trequired\n\t\t\t\t\t\t\tonChange={handleChange}\n\t\t\t\t\t\t\ttype='password'\n\t\t\t\t\t\t\tid='re_password'\n\t\t\t\t\t\t\tvalue={user.re_password}\n\t\t\t\t\t\t/>\n\t\t\t\t\t</div>\n\t\t\t\t\t<button\n\t\t\t\t\t\tonClick={handleSubmit}\n\t\t\t\t\t\ttype='submit'\n\t\t\t\t\t\tclassName='pretty-button'>\n\t\t\t\t\t\tSign up\n\t\t\t\t\t</button>\n\t\t\t\t\t{passwordError && <h3>Passwords must match.</h3>}\n\t\t\t\t\t<button className='pretty-button'>\n\t\t\t\t\t\t<Link to='/login'>Sign In</Link>\n\t\t\t\t\t</button>\n\t\t\t\t</form>\n\t\t\t</div>\n\t\t</div>\n\t);\n};\n\nexport default Register;\n","/Users/owner/Desktop/sei/unit4/project/receipts-frontend/src/Routes.js",[],"/Users/owner/Desktop/sei/unit4/project/receipts-frontend/src/components/Error/Error.js",[],{"ruleId":"63","severity":1,"message":"64","line":5,"column":8,"nodeType":"65","messageId":"66","endLine":5,"endColumn":11},{"ruleId":"63","severity":1,"message":"67","line":1,"column":8,"nodeType":"65","messageId":"66","endLine":1,"endColumn":13},{"ruleId":"63","severity":1,"message":"68","line":2,"column":8,"no